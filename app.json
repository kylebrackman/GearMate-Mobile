{
  "expo": {
    "name": "GM-Mobile",
    "slug": "GM-Mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "gearmate-mobile",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "bundleIdentifier": "com.gearmate.dev",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
            ]
          }
        ]
      },
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gearmate.dev",
      "googleServicesFile": "./google-services.json"
    },
    "extra": {
      "appleServiceId": "com.gearmate.devServiceID",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0b029570-03dd-47ca-88a6-bd013a9fe0e7"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-apple-authentication",
      "@react-native-google-signin/google-signin",
      "expo-dev-client"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}

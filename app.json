{
  "expo": {
    "name": "GM-Mobile",
    "slug": "GM-Mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "gearmate-mobile",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "bundleIdentifier": "com.gearmate.dev",
      "googleServicesFile": "./GoogleService-Info.plist",
      "config": {
        "usesSignInWithApple": true,
        "clientId": "com.gearmate.dev"
      },
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": []
          }
        ]
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.gearmate.dev"
    },
    "extra": {
      "appleServiceId": "com.gearmate.devServiceID",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0b029570-03dd-47ca-88a6-bd013a9fe0e7"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-apple-authentication",
      "@react-native-firebase/app",
      "expo-dev-client",
      "@react-native-google-signin/google-signin",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "kylebrackman"
  }
}

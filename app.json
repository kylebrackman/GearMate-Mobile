{
  "expo": {
    "name": "GM-Mobile",
    "slug": "GM-Mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "gearmate-mobile",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "bundleIdentifier": "gearmate.dev",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": []
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gearmate.dev"
    },
    "extra": {
      "appleServiceId": "com.gearmate.devServiceID",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "bb7b7a91-1ef0-48c8-bef7-a9c0bcea6625"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-apple-authentication",
      "expo-dev-client",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "kylebrackman"
  }
}
